    "react-app-polyfill": "^0.2.0",
    "react-chartjs-2": "^2.7.4",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.7.0",
    "react-leaflet": "^2.2.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
